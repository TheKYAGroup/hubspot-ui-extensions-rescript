{
  "name": "hubspot-ui-extensions-rescript",
  "jsx": { "version": 4 },
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "rescript-webapi",
    "@glennsl/rescript-fetch"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "gentypeconfig": {
    "allowJs": true,
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx"
  }
}

